	.EQU	@, $49
	;.EQU	@, PASSWORD
	.CHAR 'get pwned', LENGTH
	.EQU	@, $000	
	.EQU	KEYBOARD_STATUS, $000
	.EQU	KEYBOARD_DATA, $001
	LDA#	0
	LDF#	0
	LDX#	0
KEYPOLL:	INB	KEYBOARD_STATUS
	NOP
	JGE	KEYPOLL
	INB	KEYBOARD_DATA
	STA ! $D10
	ADF#	1
	LDX#	0
WORDCHECK:	LDC	$49
	CMA+ ! $D10-LENGTH
	JNE	KEYPOLL
	AOC#	LENGTH
	JEQ	DELETE_RESET
	JMP	WORDCHECK

DELETE_RESET:	LDX#	0
	LDA#	0

D_LOOP:	STA+	0
	AOC#	50
	JGE	END
	JMP	D_LOOP
END:	HLT